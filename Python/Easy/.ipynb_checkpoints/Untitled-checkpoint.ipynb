    "def xorOperation(n, start):\n",
    "    \n",
    "    res_num = [start]\n",
    "    \n",
    "    for i in range(1,n):\n",
    "        \n",
    "        res_num.append(start+ i*2)\n",
    "    return res_num[-1]\n",
    "    \n",
    "xorOperation(n = 5, start = 0)"
   ]
